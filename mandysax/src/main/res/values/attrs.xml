<?xml version="1.0" encoding="utf-8"?>
<resources>
	
    <declare-styleable name="MusicView">  
        <attr name="textPrompt" format="string"/>  
        <attr name="textColor" format="color"/>  
	</declare-styleable>
    
	<declare-styleable name="FragmentPage">  
        <attr name="navigationbar_top" format="boolean"/>  
        <attr name="always_show" format="boolean"/>  
		<attr name="always_bottom" format="boolean"/>
	</declare-styleable>
	
    <declare-styleable name="BottomNavigationBar">
        <attr name="show_type" format="enum">
            <enum name="normal" value="0"></enum>
            <enum name="no_text" value="1"></enum>
            <enum name="no_image" value="2"></enum>
            <enum name="check_show_text" value="3"></enum>
        </attr>
        <attr name="textCheckedColor" format="color"/>
        <attr name="textUnCheckedColor" format="color"/>
    </declare-styleable>
	
</resources>
