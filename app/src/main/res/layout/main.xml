<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent">

	<mandysax.Design.BottomNavigationBar
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:id="@+id/mainBottomNavigationBar1"/>

	<mandysax.Design.MusicView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:id="@+id/mainMusicView1"
		android:layout_above="@id/mainBottomNavigationBar1"/>

	<mandysax.Design.FragmentPage
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_above="@id/mainMusicView1"
		android:id="@+id/mainFragmentPage1"/>

</RelativeLayout>

